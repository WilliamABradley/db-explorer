{
  "name": "db_explorer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm run action:monaco",
    "action:monaco": "copyfiles -u 1 -s \"node_modules/monaco-editor/**\" assets/editor.bundle && rimraf assets/editor.bundle/monaco-editor/package.json",
    "action:nodify": "npx rn-nodeify --install events,stream,process,util,assert,fs,path,dns,dgram,crypto,url,net --hack",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "windows": "react-native run-windows --singleproc",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "metro-config": "^0.66.2",
    "react": "^17.0.2",
    "react-native": "^0.65.1",
    "react-native-table-component": "^1.2.1",
    "react-native-webview": "^11.13.0",
    "react-native-windows": "^0.65.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.18",
    "@types/react-native": "^0.64.13",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.24.0",
    "jest": "^27.0.6",
    "metro-react-native-babel-preset": "^0.66.2",
    "monaco-editor": "^0.27.0",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "rimraf": "^3.0.2",
    "rn-nodeify": "^10.3.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
